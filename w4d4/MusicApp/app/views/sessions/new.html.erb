<h1>Sign In!</h1>

<form action=" <%= session_url %>" method="POST">
  <%= auth_token %>

  <label for="user_email">Email Address:</label>
  <input type="text"
         name="user[email]"
         id="user_email">

  <br/>
  <label for="user_password">Password:</label>
  <input type="text"
         name="user[password]"
         id="user_password">

  <br/>
  <input type="submit" value="Sign In!">
</form>
